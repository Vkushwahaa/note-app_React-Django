{
  "version": 2,
  "builds": [
    {
      "src": "note_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "installCommand": ""
      }
    },
    {
      "src": "frontend_note-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm install && npm run build",
        "routes": [{ "src": "/(.*)", "dest": "frontend_note-app/dist/$1" }]
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/(.*)", "dest": "note_app/wsgi.py" }
  ]
}
