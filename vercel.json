{
  "builds": [
    {
      "src": "note_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "installCommand": ""
      }
    },
    {
      "src": "frontend_note-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/assets",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "note_app/wsgi.py"
    }
  ]
}
