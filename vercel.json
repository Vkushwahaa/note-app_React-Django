{
  "version": 2,
  "builds": [
    {
      "src": "note_app/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "note_app/wsgi.py"
    }
  ],
  "hooks": {
    "post-build": "python3 manage.py migrate"
  }
}
